<%= form_for @address, url: profile_address_path(@address) do |f| %>
<%= f.label :address_line %>
<%= f.text_field :address_line %>
<%= f.label :city %>
<%= f.text_field :city %>
<%= f.label :state %>
<%= f.text_field :state %>
<%= f.label :zip%>
<%= f.text_field :zip%>
<%= f.label :nickname %>
<%= f.text_field :nickname %>
<%= f.submit %>
<% end %>
